{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@5.5.0",
      "maxDuration": 10,
      "includeFiles": "shared/**"
    }
  },
  "rewrites": [
    {
      "source": "/((?!api|_next|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
